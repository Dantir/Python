import math
def function(num1,num2):
    num = math.sqrt(int(num1)*int(num2))
    return num
def main():
    print(function(input("Please write the first number: "),input("Please write the second number: ")))
    #num2=int(input("Please write the second number: "))
    #print(function(input("The geometric mean of it is : "))
main()
